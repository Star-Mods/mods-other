    <StateGroup name="ClassicIconRacialTint">
        <DefaultState val="Default"/>
        <State name="ClassicTint">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="True"/>
            <When type="Property" frame="NormalImage" Desaturated="True"/>
            <When type="Property" frame="NormalImage" DesaturationColor="ffffff" operator="NotEqual"/>
            <When type="Property" frame="NormalImage" DesaturationColor="128,128,128" operator="NotEqual"/>
            <When type="Property" frame="NormalImage" DesaturationColor="77,77,77" operator="NotEqual"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="NormalImage_Classic" Desaturated="True"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="HoverImage_Classic" Desaturated="True"/>
        </State>
        <State name="Tint">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="False"/>
            <When type="Property" frame="NormalImage" Desaturated="True"/>
            <When type="Property" frame="NormalImage" DesaturationColor="ffffff" operator="NotEqual"/>
            <When type="Property" frame="NormalImage" DesaturationColor="128,128,128" operator="NotEqual"/>
            <When type="Property" frame="NormalImage" DesaturationColor="77,77,77" operator="NotEqual"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="NormalImage_Classic" Desaturated="True"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="HoverImage_Classic" Desaturated="True"/>
        </State>
        <State name="Normal">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="False"/>
            <When type="Property" frame="NormalImage" Desaturated="False"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="NormalImage_Classic" Desaturated="True"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="HoverImage_Classic" Desaturated="True"/>
        </State>
        <State name="NormalActive">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="False"/>
            <When type="Property" frame="NormalImage" Desaturated="True"/>
            <When type="Property" frame="NormalImage" DesaturationColor="ffffff"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="NormalImage_Classic" Desaturated="True"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DesaturationColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="HoverImage_Classic" Desaturated="True"/>
        </State>
        <State name="Default"/>
    </StateGroup>

    <StateGroup name="ClassicIconTinting">
        <DefaultState val="False"/>
        <State name="TrueActiveCommand">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="True"/>
            <When type="StateGroup" ClassicIconState="Highlight"/>
            <When type="Property" frame="$ancestor[@type=CommandPanel]" Enabled="True"/>
            <When type="Property" frame="NormalImage" Enabled="True"/>
            <Action type="SetProperty" frame="NormalImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="NormalImage_Classic" LightAdjustColor="###ClassicButtonIconColor"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DarkAdjustColor="###ClassicButtonIconColor_Clicked"/>
            <Action type="SetProperty" frame="HoverImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="HoverImage_Classic" LightAdjustColor="###ClassicButtonIconColor"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DarkAdjustColor="###ClassicButtonIconColor_Clicked"/>
        </State>
        <State name="True">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="True"/>
            <When type="StateGroup" ClassicIconState="Normal"/>
            <When type="Property" frame="$ancestor[@type=CommandPanel]" Enabled="True"/>
            <When type="Property" frame="NormalImage" Enabled="True"/>
            <Action type="SetProperty" frame="NormalImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="NormalImage_Classic" LightAdjustColor="###ClassicButtonIconColor"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DarkAdjustColor="###ClassicButtonIconColorDark"/>
            <Action type="SetProperty" frame="HoverImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="HoverImage_Classic" LightAdjustColor="###ClassicButtonIconColor"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DarkAdjustColor="###ClassicButtonIconColorDark"/>
        </State>
        <State name="Disabled">
            <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
            <When type="StateGroup" frame="DummyBox" ClassicIcon="True"/>
            <Action type="SetProperty" frame="NormalImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="NormalImage_Classic" LightAdjustColor="128,128,128"/>
            <Action type="SetProperty" frame="NormalImage_Classic" DarkAdjustColor="128,128,128"/>
            <Action type="SetProperty" frame="HoverImage_Classic" ColorAdjustMode="Colorize"/>
            <Action type="SetProperty" frame="HoverImage_Classic" LightAdjustColor="128,128,128"/>
            <Action type="SetProperty" frame="HoverImage_Classic" DarkAdjustColor="128,128,128"/>
        </State>
        <State name="False">
            <Action type="SetProperty" frame="NormalImage_Classic" ColorAdjustMode="None"/>
            <Action type="SetProperty" frame="HoverImage_Classic" ColorAdjustMode="None"/>
        </State>
    </StateGroup>

    <Frame type="Image" name="NormalImage_ClassicTemplate">
        <Anchor relative="$parent/NormalImage"/>
        <Width val="{$parent/NormalImage/@Width}"/>
        <Height val="{$parent/NormalImage/@Height}"/>
        <Enabled val="{$parent/NormalImage/@Enabled}"/>
        <LayerCount val="1"/>
        <Texture val="{$parent/NormalImage/@Texture[0]}" layer="0"/>
        <TextureType val="{$parent/NormalImage/@TextureType[0]}" layer="0"/>
        <RenderPriority val="511"/>

        <Visible val="False"/>

        <StateGroup name="Visible">
            <DefaultState val="Default"/>
        
            <State name="True">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="Property" frame="$parent" Hovering="False"/>
                <When type="Property" frame="$parent/NormalImage" Texture="" index="0" operator="NotEqual"/>
                <Action type="SetProperty" frame="$this" Visible="True"/>
            </State>
        
            <State name="False">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="Property" frame="$parent" Hovering="False"/>
                <When type="Property" frame="$parent/NormalImage" Texture="" index="0"/>
                <Action type="SetProperty" frame="$this" Visible="False"/>
            </State>

            <State name="Default">
                <Action type="SetState" frame="$this" group="ForceGroup" state="Default"/>
            </State>
        </StateGroup>

        <StateGroup name="ForceGroup">
            <DefaultState val="Default"/>
            <State name="Set_0">
                <Action type="SetProperty" frame="$parent/NormalImage" LayerAlpha="0" index="0"/>
            </State>
            <State name="Set_1">
                <Action type="SetProperty" frame="$parent/NormalImage" LayerAlpha="0" index="0"/>
            </State>
            <State name="Default"/>
        </StateGroup>

        <Animation name="StateRepeat">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>
            <Event event="OnHidden" action="Stop" frame="$this"/>
            <Controller type="State" stateGroup="ForceGroup" end="Loop">
                <Key type="Identifier" time="0" value="Set_0"/>
                <Key type="Identifier" time="0" value="Set_1"/>
            </Controller>
        </Animation>

    </Frame>

    <Frame type="Image" name="HoverImage_ClassicTemplate">
        <Anchor relative="$parent/HoverImage"/>
        <Width val="{$parent/HoverImage/@Width}"/>
        <Height val="{$parent/HoverImage/@Height}"/>
        <Enabled val="{$parent/HoverImage/@Enabled}"/>
        <LayerCount val="1"/>
        <Texture val="{$parent/HoverImage/@Texture[0]}" layer="0"/>
        <TextureType val="{$parent/HoverImage/@TextureType[0]}" layer="0"/>
        <RenderPriority val="511"/>

        <Visible val="False"/>
        
        <StateGroup name="Visible">
            <DefaultState val="Default"/>
        
            <State name="True">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="Property" frame="$parent" Hovering="True"/>
                <When type="Property" frame="$parent/HoverImage" Texture="" index="0" operator="NotEqual"/>
                <Action type="SetProperty" frame="$this" Visible="True"/>
            </State>
            
            <State name="False">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="Property" frame="$parent" Hovering="False"/>
                <When type="Property" frame="$parent/HoverImage" Texture="" index="0"/>
                <Action type="SetProperty" frame="$this" Visible="False"/>
            </State>

            <State name="Default">
                <Action type="SetState" frame="$this" group="ForceGroup" state="Default"/>
            </State>
        </StateGroup>

        <StateGroup name="ForceGroup">
            <DefaultState val="Default"/>
            <State name="Set_0">
                <Action type="SetProperty" frame="$parent/HoverImage" LayerAlpha="0" index="0"/>
            </State>
            <State name="Set_1">
                <Action type="SetProperty" frame="$parent/HoverImage" LayerAlpha="0" index="0"/>
            </State>
            <State name="Default"/>
        </StateGroup>

        <Animation name="StateRepeat">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>
            <Event event="OnHidden" action="Stop" frame="$this"/>
            <Controller type="State" stateGroup="ForceGroup" end="Loop">
                <Key type="Identifier" time="0" value="Set_0"/>
                <Key type="Identifier" time="0" value="Set_1"/>
            </Controller>
        </Animation>

    </Frame>

    <Frame type="EquipmentIcon" name="EquipmentIconTemplate" file="EquipmentIcon">

        <StateGroup name="ClassicIcon">
            <DefaultState val="False"/>
        
            <State name="True">
                <When type="StateGroup" frame="ClassicIconListenerBox" ClassicIcon="True"/>
                <Action type="SetProperty" frame="$this" TextureCoords="-0.07,-0.07,1.07,1.07"/>
            </State>
        
            <State name="False">
                <Action type="SetProperty" frame="$this" TextureCoords="0,0,1,1"/>
            </State>
        </StateGroup>
        
        <StateGroup name="ClassicTint">
            <DefaultState val="False"/>

            <State name="True">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="StateGroup" frame="$this" ClassicIcon="False"/>
                <Action type="SetProperty" frame="$this" DesaturationColor="###ClassicButtonIconColor"/>
                <Action type="SetProperty" frame="$this" Desaturated="True"/>
            </State>
        
            <State name="False"/>
        </StateGroup>

        <Frame type="EditBox" name="ClassicIconListenerBox" template="SCBW_ClassicIcons/ListenerBoxTemplate">
            <MaxCharacters val="29"/>
            <Text val="{$parent/@Texture}"/>
            <StateGroup name="ClassicIcon" template="SCBW_ClassicIcons/ClassicIcon"/>
        </Frame>
    </Frame>

    <Frame type="Image" name="AlertItemTemplate/AlertIcon" file="AlertPanel">
        <StateGroup name="ClassicIconTinting">
            <DefaultState val="False"/>
            <State name="True">
                <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                <When type="StateGroup" frame="DummyBox" ClassicIcon="True"/>
                <When type="Property" frame="$ancestor[@type=AlertPanel]" Enabled="true"/>
                <Action type="SetProperty" frame="$this" Desaturated="False"/>
                <Action type="SetProperty" frame="$this" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="$this" LightAdjustColor="###ClassicButtonIconColor"/>
                <Action type="SetProperty" frame="$this" DarkAdjustColor="###ClassicButtonIconColorDark"/>
            </State>
            
            <State name="False">
                <Action type="SetProperty" frame="$this" ColorAdjustMode="None"/>
                <Action type="SetProperty" frame="$this" Desaturated="True"/>
            </State>
        </StateGroup>

        <Frame type="EditBox" name="DummyBox" template="SCBW_ClassicIcons/ListenerBoxTemplate">
            <MaxCharacters val="29"/>
            <Text val="{$parent/@Texture}"/>
            <StateGroup name="ClassicIcon" template="SCBW_ClassicIcons/ClassicIcon"/>
        </Frame>
    </Frame>

    <Frame type="QueueButton" name="QueueButtonTemplate">
        <Frame type="Image" name="NormalImage">
            <AdjustmentColor val="0,255,255,255"/>
            <ColorAdjustMode val="None"/>

            <Desaturated val="False"/>
            <DesaturationColor val="###ClassicButtonIconColorDark"/>

            <StateGroup name="ClassicIconTinting">
                <DefaultState val="False"/>
                <State name="True">
                    <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                    <When type="Property" frame="$ancestor[@type=InfoPanel]" Enabled="True"/>
                    <Action type="SendEvent" frame="$this" event="Adjust" on="Enter" undo="False"/>
                </State>
                <State name="Disabled">
                    <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                    <When type="Property" frame="$ancestor[@type=InfoPanel]" Enabled="False"/>
                    <Action type="SendEvent" frame="$this" event="Desaturate" on="Enter" undo="False"/>
                </State>
                <State name="False">
                    <Action type="SendEvent" frame="$this" event="Stop" on="Enter" undo="False"/>
                </State>
            </StateGroup>

            <Animation name="ColorAdjustment">
                <Event event="Stop" action="Stop" frame="$this"/>
                <Event event="Adjust" action="Reset,Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="Colorize"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="###ClassicButtonIconColorDarker"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
            </Animation>

            <Animation name="Desaturated">
                <Event event="Stop" action="Stop" frame="$this"/>
                <Event event="Desaturate" action="Reset,Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="Colorize"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="85,85,85"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
            </Animation>

            <Animation name="ClearAdjustment">
                <Event event="Stop" action="Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="None"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0,255,255,255"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="False"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <AdjustmentColor val="0,255,255,255"/>
            <ColorAdjustMode val="None"/>

            <Desaturated val="False"/>
            <DesaturationColor val="###ClassicButtonIconColorDark"/>

            <StateGroup name="ClassicIconTinting">
                <DefaultState val="False"/>
                <State name="True">
                    <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                    <When type="Property" frame="$ancestor[@type=InfoPanel]" Enabled="True"/>
                    <Action type="SendEvent" frame="$this" event="Adjust" on="Enter" undo="False"/>
                </State>
                <State name="Disabled">
                    <When type="AnimationState" frame="$ancestor[@name=UIContainer]" UseClassicIcons="True"/>
                    <When type="Property" frame="$ancestor[@type=InfoPanel]" Enabled="False"/>
                    <Action type="SendEvent" frame="$this" event="Desaturate" on="Enter" undo="False"/>
                </State>
                <State name="False">
                    <Action type="SendEvent" frame="$this" event="Stop" on="Enter" undo="False"/>
                </State>
            </StateGroup>

            <Animation name="ColorAdjustment">
                <Event event="Stop" action="Stop" frame="$this"/>
                <Event event="Adjust" action="Reset,Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="Colorize"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="###ClassicButtonIconColorDarker"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
            </Animation>

            <Animation name="Desaturated">
                <Event event="Stop" action="Stop" frame="$this"/>
                <Event event="Desaturate" action="Reset,Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="Colorize"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="85,85,85"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="True"/>
                </Controller>
            </Animation>

            <Animation name="ClearAdjustment">
                <Event event="Stop" action="Play" frame="$this"/>
                <Controller type="Property" end="Pause" frame="$this" property="ColorAdjustMode">
                    <Key type="Property" time="0.0" value="None"/>
                </Controller>
                <Controller type="AdjustmentColor" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0,255,255,255"/>
                </Controller>
                <Controller type="Desaturation" end="Pause" frame="$this">
                    <Key type="Bool" time="0.0" value="False"/>
                </Controller>
            </Animation>
        </Frame>


    </Frame>

